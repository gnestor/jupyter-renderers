{
  "name": "@jupyterlab/plotly-extension",
  "version": "0.13.0",
  "description": "Plotly renderer for JupyterLab",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^1.0.3",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "plotly.js": "^1.28.3"
  },
  "devDependencies": {
    "@types/plotly.js": "^1.28.3",
    "rimraf": "~2.6.2",
    "typescript": "~2.6.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyter-renderers.git"
  },
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyter-renderers/issues"
  },
  "homepage": "https://github.com/jupyterlab/jupyter-renderers"
}
